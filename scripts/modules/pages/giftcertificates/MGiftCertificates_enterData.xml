<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <description>enters data in the input fields from the gift certificates page</description>
  <parameter name="firstNameBuyer"/>
  <parameter name="lastNameBuyer"/>
  <parameter name="firstNameRecipient"/>
  <parameter name="lastNameRecipient"/>
  <parameter name="emailRecipient"/>
  <parameter name="giftCertificateValue"/>
  <parameter name="purchaseMessage"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_from" value="@{firstNameBuyer} @{lastNameBuyer}"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_recipient" value="@{firstNameRecipient} @{lastNameRecipient}"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_recipientEmail" value="@{emailRecipient}"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_confirmRecipientEmail" value="@{emailRecipient}"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_amount" value="@{giftCertificateValue}"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_message" value="@{purchaseMessage}"/>
</scriptmodule>