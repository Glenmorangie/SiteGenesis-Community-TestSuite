<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <description>enters data in the input fields from the gift certificates page</description>
  <parameter name="firstName_buyer"/>
  <parameter name="lastName_buyer"/>
  <parameter name="firstName_receiver"/>
  <parameter name="lastName_receiver"/>
  <parameter name="email_receiver"/>
  <parameter name="giftCertificateValue"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_from" value="@{firstName_buyer} @{lastName_buyer}"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_recipient" value="@{firstName_receiver} @{lastName_receiver}"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_recipientEmail" value="@{email_receiver}"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_confirmRecipientEmail" value="@{email_receiver}"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_amount" value="@{giftCertificateValue}"/>
  <command name="type" target="id=dwfrm_giftcert_purchase_message" value="have Fun"/>
</scriptmodule>