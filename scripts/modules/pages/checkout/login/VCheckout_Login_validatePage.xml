<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <description>Validate checkout start page.</description>
  <codecomment>Validate browser check</codecomment>
  <command name="waitForElementPresent" disabled="true" target="css=#main #browser-check"/>
  <command name="waitForElementPresent" target="css=#primary .checkoutlogin"/>
  <codecomment>Validate breadcrumb</codecomment>
  <command name="assertElementPresent" disabled="true" target="css=#main .breadcrumb [title=&quot;Home&quot;]"/>
  <command name="assertElementPresent" disabled="true" target="css=#main .breadcrumb [title=&quot;My Account&quot;]"/>
  <codecomment>Validate primary</codecomment>
  <command name="assertElementPresent" disabled="true" target="css=#primary h1"/>
  <command name="assertElementPresent" target="css=#primary .col-1"/>
  <command name="assertElementPresent" target="css=#primary .col-2"/>
  <codecomment>Validate primary guest or new customers</codecomment>
  <command name="assertElementPresent" target="css=#primary .checkoutlogin .col-1 .login-box h2"/>
  <codecomment>---</codecomment>
  <command name="assertElementPresent" target="css=#primary .checkoutlogin .col-1 .login-box .login-box-content p:first-of-type"/>
  <command name="assertElementPresent" target="css=#primary .checkoutlogin .col-1 .login-box .login-box-content form fieldset .form-row.formbuttonrow button"/>
  <codecomment>---</codecomment>
  <command name="assertElementPresent" target="css=#primary .checkoutlogin .col-1 .login-box .login-box-content p:last-of-type"/>
  <command name="assertElementPresent" target="css=#primary .checkoutlogin .col-1 .login-box .login-box-content form fieldset .form-row-button button"/>
  <codecomment>---</codecomment>
  <command name="assertElementPresent" target="css=#primary .checkoutlogin .col-1 .login-box div a.tooltip"/>
  <codecomment>Validate primary returning customer</codecomment>
  <command name="assertElementPresent" target="css=#primary .checkoutlogin .col-2 .login-account h2"/>
  <codecomment>---</codecomment>
  <command name="assertElementPresent" target="css=#dwfrm_login fieldset div:first-of-type label span:last-of-type"/>
  <command name="assertElementPresent" target="css=#dwfrm_login.clearfix fieldset div.form-row.required input.email-input"/>
  <codecomment>---</codecomment>
  <command name="assertElementPresent" target="css=#dwfrm_login.clearfix fieldset div:nth-of-type(2) label span:last-of-type"/>
  <command name="assertElementPresent" target="css=#dwfrm_login_password"/>
  <codecomment>---</codecomment>
  <command name="assertElementPresent" target="css=#dwfrm_login [type=&quot;Submit&quot;]"/>
  <command name="assertElementPresent" target="css=#dwfrm_login_rememberme"/>
  <codecomment>---</codecomment>
  <command name="assertElementPresent" target="css=#password-reset"/>
  <command name="assertElementPresent" target="css=#dwfrm_oauthlogin"/>
  <codecomment>Validate secondary navigation and help</codecomment>
  <command name="assertElementPresent" disabled="true" target="css=#main #secondary nav .secondary-navigation .content-asset"/>
  <command name="assertElementPresent" disabled="true" target="css=#main #secondary .account-nav-asset"/>
</scriptmodule>