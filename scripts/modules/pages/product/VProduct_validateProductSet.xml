<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <codecomment>Product Name</codecomment>
  <command name="assertElementPresent" target="css=#pdpMain.pdp-main h1.product-name[itemprop='name']"/>
  <codecomment>Product Image</codecomment>
  <command name="assertElementPresent" target="css=#pdpMain div.product-col-1 div.product-primary-image a.product-image"/>
  <codecomment>Product Number</codecomment>
  <command name="assertElementPresent" target="css=#pdpMain div.product-col-2 div.product-number span[itemprop='productID']"/>
  <codecomment>Item List</codecomment>
  <command name="assertElementPresent" target="css=#product-set-list"/>
  <command name="assertElementPresent" target="css=#product-set-list div:first-of-type .product-set-image"/>
  <command name="assertElementPresent" target="css=#product-set-list div:first-of-type .product-set-details"/>
  <codecomment>First Product</codecomment>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(1) div.product-set-image"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(1) .product-name"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(1) .product-number"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(1) .availability"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(1) .product-variations"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(1) .block-add-to-cart"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(1) .block-add-to-cart form .add-to-cart"/>
  <codecomment>Second Product</codecomment>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(2) div.product-set-image"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(2) .product-name"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(2) .product-number"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(2) .availability"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(2) .product-variations"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(2) .block-add-to-cart"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(2) .block-add-to-cart form .add-to-cart"/>
  <codecomment>Third Product</codecomment>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(3) div.product-set-image"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(3) .product-name"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(3) .product-number"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(3) .availability"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(3) .product-variations"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(3) .block-add-to-cart"/>
  <command name="assertElementPresent" target="css=#product-set-list div:nth-of-type(3) .block-add-to-cart form .add-to-cart"/>
  <codecomment>Add All To Cart Button</codecomment>
  <command name="assertElementPresent" target="css=#add-all-to-cart:not(disabled)"/>
</scriptmodule>