<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <description>Validate wishlist page.</description>
  <command name="waitForText" target="css=#primary h1" value="*Find Someone's Wish List*"/>
  <command name="assertElementPresent" target="css=#primary #dwfrm_wishlist_search #dwfrm_wishlist_search_lastname"/>
  <command name="assertElementPresent" target="css=#primary #dwfrm_wishlist_search #dwfrm_wishlist_search_firstname"/>
  <command name="assertElementPresent" target="css=#primary #dwfrm_wishlist_search #dwfrm_wishlist_search_email"/>
  <command name="assertElementPresent" target="css=#primary #dwfrm_wishlist_search button[name=&quot;dwfrm_wishlist_search_search&quot;][type=&quot;submit&quot;]"/>
  <command name="assertElementPresent" target="css=#primary #dwfrm_wishlist_shipping button[name=&quot;dwfrm_wishlist_addGiftCertificate&quot;]"/>
  <command name="assertElementPresent" target="css=#primary #dwfrm_wishlist_share button[name=&quot;dwfrm_wishlist_setListPublic&quot;]"/>
  <codecomment>--- share buttons ---</codecomment>
  <command name="assertElementPresent" target="css=#dwfrm_wishlist_share div .fa-facebook-square"/>
  <command name="assertElementPresent" target="css=#dwfrm_wishlist_share div .fa-twitter-square"/>
  <command name="assertElementPresent" target="css=#dwfrm_wishlist_share div .fa-google-plus-square"/>
  <command name="assertElementPresent" target="css=#dwfrm_wishlist_share div .fa-pinterest-square"/>
  <command name="assertElementPresent" target="css=#dwfrm_wishlist_share div .fa-envelope-square"/>
  <command name="assertElementPresent" target="css=#dwfrm_wishlist_share div .fa-link"/>
</scriptmodule>