<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <description>Navigates to a third level category by using name on the left secondary navigation.</description>
  <parameter name="nameLevel3" desc="Link name for level 3"/>
  <command name="clickAndWait" target="xpath=id('category-level-2')/li/a[contains(., '@{nameLevel3}')]"/>
</scriptmodule>